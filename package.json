{
    "name": "epress",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "node -r esm ./app/commands/server",
        "watch": "nodemon -x 'node -r esm' --watch app --ignore app/client -L -e js,es,jsx ./app/commands/server",
        "build": "npm run build-locale && npm run build-client",
        "build-client": "webpack --config app/client/webpack.config.js",
        "build-locale": "extract-messages -l 'zh-CN' -d 'zh-CN' -o app/client/locales --flat 'app/client/**/*.jsx'",
        "test": "export NODE_ENV=test && standard ./app && npm run resetup && jest",
        "setup": "npm run knex migrate:latest && npm run knex seed:run",
        "resetup": "npm run knex migrate:rollback && npm run setup",
        "knex": "knex --knexfile ./deploy/database/knexfile.js"
    },
    "repository": {
        "type": "git",
        "url": "ssh://git@dev.dianpu.io:30022/garbin/ryzens-example.git"
    },
    "standard": {
        "parser": "babel-eslint",
        "ignore": [
            "/data"
        ]
    },
